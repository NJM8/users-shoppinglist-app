extends base

block content
  h2 Hello #{admin.username}

  ul
  for user in allUsers
    li #{user.username} 
    form(action=`/users/admin/${admin.id}/deleteUser/${user.id}?_method=DELETE` method="POST")
      input(type="submit" value="Delete user")


  form(action=`/users/${admin.id}`)
    input(type="submit" value="Back to your page")